	<queryString>
		<![CDATA[SELECT TR.TRX_VOUCHER_ID,
TRA.TRX_COVER_CODE,
(SELECT MA12.SA_NAME FROM MA0012 MA12 WHERE MA12.SA_CODE = TR.TRX_OFFICER) OFFICER,
MA.TC_DESC TYPE_OF_COVER,
TRA.TRX_REMARKS POLICY_NO,
TR.TRX_INSURED_NAME,
case when (TRA.TRX_INS_START = TRA.TRX_INS_END and TRA.TRX_INS_DUR = 0)	then 'TBA'
when (TRA.TRX_INS_START is not null and TRX_INS_END is not null)
then concat(date_format(TRA.TRX_INS_START,'%d %M %Y'), ' - ' , date_format(TRA.TRX_INS_END,'%d %M %Y'))
when TRA.TRX_INS_DUR > 0
then concat(TRA.TRX_INS_DUR, ' day(s) from ' , date_format(TRA.TRX_INS_START,'%d %M %Y'))end PERIOD,
TRA.TRX_INCEP_DATE,
TRA.TRX_INS_INSURED INTEREST_INSURED,
TRA.TRX_CURR_ID CURR,
TRA.TRX_SUM_INSURED,
TRA.TRX_INS_LOCATION RISK_LOCATION,
TR.TRX_PAY_MTHD PREMIUM_PAYMENT,
TR.TRX_PERIOD_VLD VALIDATY,
TRC.TRX_PREMIUM_SELL PREMIUM,
(TRC.TRX_POL_AMOUNT + TRC.TRX_OTHERS2_AMOUNT) POLICY_COST,
(TRC.TRX_SDUTY_AMOUNT + TRC.TRX_OTHERS3_AMOUNT) STAMP_DUTY,
TRC.TRX_OTHERS1_AMOUNT OTHER_AMT,
TRC.TRX_BANK_AMOUNT BANK_FEE,
TRC.TRX_DISC_SELL DISCOUNT,
(TRC.TRX_ADMIN_AMOUNT - TRC.TRX_COM_AMOUNT + TRC.TRX_OTHERS4_AMOUNT + TRC.TRX_BANK_AMOUNT) ADMIN_FEE,
TRC.TRX_BRKR_FEE BROKER_FEE,
TRA.TRX_TRX_ID,
MA5.CLI_NAME ,
MA5.CLI_ADDRESS,
M10.CO_NAME,
M10.CO_ADDRESS,
M10.CO_PIC_TELP,
M10.CO_PIC_EMAIL,
M10.CO_LOGO,
MA.TC_SALES_TAX IS_PPN,
(SELECT PA_CHILD_VALUE FROM MA0014 WHERE PA_CHILD_CODE = 'TAXRATEH003') PPN
FROM TR0006 TR
JOIN TR0006A TRA ON TRA.TRX_VOUCHER_ID = TR.TRX_VOUCHER_ID
JOIN TR0006C TRC ON TRC.TRX_VOUCHER_ID = TR.TRX_VOUCHER_ID
JOIN MA0011 MA ON MA.TC_CODE = TRA.TRX_COVER_CODE
JOIN MA0005 MA5 ON MA5.CLI_CODE = TR.TRX_CLIENT
join MA0010 M10
WHERE TR.TRX_VOUCHER_ID = $P{P_VOUCHER_ID}
AND TR.TRX_CLIENT = $P{P_TRX_CLIENT}
AND TR.TRX_TRX_ID = $P{P_TRX_ID}]]>
	</queryString>
