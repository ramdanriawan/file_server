	<queryString>
		<![CDATA[select date_format(T.TX_ADVICE_DATE, '%D %M %Y') TX_ADVICE_DATE,
	T.TX_CLIENT_ID,
	M5CLI.CLI_NAME CLIENT,
	$P{P_INS_ID} TRX_INS_ID,
	(select CLI_NAME FROM MA0005 where CLI_CODE = $P{P_INS_ID}) TRX_INS_NAME,
	T.TX_POLICY_NO,
	T.TX_UW_REFF,
	T.TX_ATT_CLIENT,
	T.TX_DATE_FROM,
	T.TX_DATE_TO,
	T.TX_INTEREST,
	T.TX_CURR_ID,
	(SELECT MAX(TX_MEMO) FROM TR0015A WHERE TX_VOUCHER_ID = $P{P_TX_VOUCHER_ID}
		AND $X{IN,ID_KEY,P_ID_KEY_CLAIMS}) TX_MEMO,
	(SELECT
		SUM(CASE
		 WHEN $P{P_TITLE} = 'DEFINITE LOSS ADVICE' THEN
			TX_CLAIM_DLA
		 ELSE
			TX_CLAIM_PLA
		END)
		FROM TR0015A WHERE TX_VOUCHER_ID = $P{P_TX_VOUCHER_ID}
		AND TX_CLAIM_TYPE = '0') TOTAL_CLAIMS,
	(SELECT
		SUM(CASE
		 WHEN $P{P_TITLE} = 'DEFINITE LOSS ADVICE' AND TX_CLAIM_TYPE <> '1' THEN
			TX_CLAIM_DLA
		 WHEN $P{P_TITLE} = 'DEFINITE LOSS ADVICE' AND TX_CLAIM_TYPE = '1' THEN
			TX_CLAIM_DLA * -1
		 WHEN $P{P_TITLE} = 'PRELIMINARY LOSS ADVICE' AND TX_CLAIM_TYPE <> '1' THEN
			TX_CLAIM_PLA
		 WHEN $P{P_TITLE} = 'PRELIMINARY LOSS ADVICE' AND TX_CLAIM_TYPE = '1' THEN
			TX_CLAIM_PLA * -1
		END) AMOUNT
		FROM TR0015A WHERE TX_VOUCHER_ID = $P{P_TX_VOUCHER_ID}
		AND $X{IN,ID_KEY,P_ID_KEY_CLAIMS}) TOTAL,
	(SELECT
		SUM(CASE
		 WHEN $P{P_TITLE} = 'DEFINITE LOSS ADVICE' THEN
			TX_CLAIM_DLA
		 ELSE
			TX_CLAIM_PLA
		END)
		FROM TR0015A WHERE TX_VOUCHER_ID = $P{P_TX_VOUCHER_ID}
		AND $X{IN,ID_KEY,P_ID_KEY_CLAIMS}
		AND TX_CLAIM_TYPE = '0') CLAIMS,
	(SELECT
		SUM(CASE
		 WHEN $P{P_TITLE} = 'DEFINITE LOSS ADVICE' THEN
			TX_CLAIM_DLA
		 ELSE
			TX_CLAIM_PLA
		END)
		FROM TR0015A WHERE TX_VOUCHER_ID = $P{P_TX_VOUCHER_ID}
		AND $X{IN,ID_KEY,P_ID_KEY_CLAIMS}
		AND TX_CLAIM_TYPE = '1') DEDUCTION,
	(SELECT
		SUM(CASE
		 WHEN $P{P_TITLE} = 'DEFINITE LOSS ADVICE' THEN
			TX_CLAIM_DLA
		 ELSE
			TX_CLAIM_PLA
		END)
		FROM TR0015A WHERE TX_VOUCHER_ID = $P{P_TX_VOUCHER_ID}
		AND $X{IN,ID_KEY,P_ID_KEY_CLAIMS}
		AND TX_CLAIM_TYPE = '2') SURVEY,
	(SELECT
		SUM(CASE
		 WHEN $P{P_TITLE} = 'DEFINITE LOSS ADVICE' THEN
			TX_CLAIM_DLA
		 ELSE
			TX_CLAIM_PLA
		END)
		FROM TR0015A WHERE TX_VOUCHER_ID = $P{P_TX_VOUCHER_ID}
		AND $X{IN,ID_KEY,P_ID_KEY_CLAIMS}
		AND TX_CLAIM_TYPE = '3') ADJUSTER,
	(SELECT
		SUM(CASE
		 WHEN $P{P_TITLE} = 'DEFINITE LOSS ADVICE' THEN
			TX_CLAIM_DLA
		 ELSE
			TX_CLAIM_PLA
		END)
		FROM TR0015A WHERE TX_VOUCHER_ID = $P{P_TX_VOUCHER_ID}
		AND $X{IN,ID_KEY,P_ID_KEY_CLAIMS}
		AND TX_CLAIM_TYPE = '99') OTHERS,
	T.TX_OR_AMT,
	T.TX_QS_AMT,
	T.TX_UR_AMT,
	T.TX_REINS_CL,
	T.TX_REINS_UW,
	T.TX_CLCL_AMT,
	T.TX_UWCL_AMT,
	T.TX_PARTY_NAME,
	T.TX_CAUSE,
	T.TX_LOST_DATE,
	T.TX_DETAILS,
	CO.CO_LOGO,
	CO.CO_NAME,
	CO.CO_ADDRESS from TR0015 T
	left join MA0010 CO on (1 = 1)
	left join MA0005 M5CLI on (M5CLI.CLI_CODE = T.TX_CLIENT_ID)
	where T.TX_VOUCHER_ID = $P{P_TX_VOUCHER_ID};]]>
	</queryString>
