		<queryString>
			<![CDATA[SELECT
	(CASE
		WHEN $P{P_TITLE} = 'DEFINITE LOSS ADVICE' THEN
			TX_CLAIM_DLA
		ELSE
			TX_CLAIM_PLA
	END) AMOUNT_OTHERS, TX_CLAIM_REMARKS
	FROM TR0015A WHERE TX_VOUCHER_ID = $P{P_TX_VOUCHER_ID}
	AND $X{IN,ID_KEY,P_ID_KEY_CLAIMS}
	AND TX_CLAIM_TYPE = '99'
	ORDER BY ID_KEY ASC;]]>
		</queryString>
		<field name="AMOUNT_OTHERS" class="java.math.BigDecimal"/>
		<field name="TX_CLAIM_REMARKS" class="java.lang.String">
			<fieldDescription><![CDATA[0=LEAD,1=CO INS]]></fieldDescription>
		</field>
	</subDataset>
