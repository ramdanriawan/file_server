#server config
server.port=8281
server.servlet.context-path=/app-re-broker

#naming strategy
spring.jpa.hibernate.naming.physical-strategy=com.biru.config.RealNamingStrategyImpl

spring.jpa.properties.hibernate.id.new_generator_mappings=false

#datasource
datasource.url.properties=?serverTimezone=Asia/Jakarta&useLegacyDatetimeCode=false&zeroDateTimeBehavior=convertToNull

#tenant default
tenant.id.default=CPU01

#path file report
report.path.jasper=/app-dev/report/
report.path.result=/app-dev/result/

#path file upload
document.path=/app-dev/document/

spring.mail.host=smtp.gmail.com
spring.mail.port=587
spring.mail.username=adm.rebroker@gmail.com
spring.mail.password=Password2019

# Other properties
spring.mail.properties.mail.smtp.auth=true
spring.mail.properties.mail.smtp.connectiontimeout=5000
spring.mail.properties.mail.smtp.timeout=5000
spring.mail.properties.mail.smtp.writetimeout=5000

# TLS , port 587
spring.mail.properties.mail.smtp.starttls.enable=true